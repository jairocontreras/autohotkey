<NotepadPlus>
  <UserLang name="AutoHotkey" ext="ahk" udlVersion="2.1">
    <Settings></Settings>
    <KeywordLists>
      <Keywords name="Comments">00; 01 02 03/* 04*/</Keywords>
      <Keywords name="Delimiters">00&quot; 01` 02&quot;</Keywords>
      <Keywords name="Operators"># ( ) + , . / : ^</Keywords>
      <!--directives-->
      <Keywords name="Keywords1">notrayicon</Keywords>
      <!--expressions-->
      <Keywords name="Keywords2">and catch else if or try</Keywords>
      <!--keyboard-->
      <Keywords name="Keywords3">down enter left pgdn pgup right space up</Keywords>
      <!--functions-->
      <Keywords name="Keywords4">global loop msgbox numput onmessage onexit persistent processsetpriority read return send wingetpos winmaximize winminimize winmove winrestore winsetalwaysontop</Keywords>
      <!--functions()-->
      <Keywords name="Keywords5">add buffer dllcall gui instr isinteger loadpicture numget onevent opt sendmessage show trayseticon wingetminmax wingetprocessname wingetstyle winexist</Keywords>
      <!--properties-->
      <Keywords name="Keywords6">marginy text value visible</Keywords>
      <!--values-->
      <Keywords name="Keywords7">false true</Keywords>
      <!--variables-->
      <Keywords name="Keywords8">a_cursor a_loopreadline a_screenwidth</Keywords>
    </KeywordLists>
    <Styles>
      <WordsStyle name="LINE COMMENTS" fgColor="008000"/>
      <WordsStyle name="COMMENTS" fgColor="008000" bgColor="f0f0f0"/>
      <WordsStyle name="DELIMITERS1" fgColor="ee0000"/>
      <WordsStyle name="KEYWORDS1" fgColor="8c868f"/>
      <WordsStyle name="KEYWORDS2" fgColor="ff7800"/>
      <WordsStyle name="KEYWORDS3" fgColor="b43c7a"/>
      <WordsStyle name="KEYWORDS4" fgColor="0060ff"/>
      <WordsStyle name="KEYWORDS5" fgColor="ff00ff"/>
      <WordsStyle name="KEYWORDS6" fgColor="3b5bb5"/>
      <WordsStyle name="KEYWORDS7" fgColor="cc7700"/>
      <WordsStyle name="KEYWORDS8" fgColor="3b5bb5"/>
    </Styles>
  </UserLang>
</NotepadPlus>